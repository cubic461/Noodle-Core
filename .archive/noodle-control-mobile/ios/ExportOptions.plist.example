<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Export method: app-store, ad-hoc, development, enterprise -->
    <key>method</key>
    <string>app-store</string>
    
    <!-- Team ID from Apple Developer Portal -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>
    
    <!-- Upload symbols to Apple for crash reporting -->
    <key>uploadSymbols</key>
    <true/>
    
    <!-- Upload bitcode for App Store (deprecated but still required for some apps) -->
    <key>uploadBitcode</key>
    <false/>
    
    <!-- Compile Bitcode -->
    <key>compileBitcode</key>
    <false/>
    
    <!-- Thinning -->
    <key>thinning</key>
    <string><none></string>
    
    <!-- For Ad-Hoc distribution -->
    <!-- 
    <key>provisioningProfiles</key>
    <dict>
        <key>com.noodlecontrol.app</key>
        <string>YOUR_AD_HOC_PROVISIONING_PROFILE</string>
    </dict>
    -->
    
    <!-- For Development distribution -->
    <!-- 
    <key>provisioningProfiles</key>
    <dict>
        <key>com.noodlecontrol.app</key>
        <string>YOUR_DEVELOPMENT_PROVISIONING_PROFILE</string>
    </dict>
    -->
    
    <!-- For Enterprise distribution -->
    <!-- 
    <key>provisioningProfiles</key>
    <dict>
        <key>com.noodlecontrol.app</key>
        <string>YOUR_ENTERPRISE_PROVISIONING_PROFILE</string>
    </dict>
    -->
    
    <!-- Signing certificate -->
    <key>signingCertificate</key>
    <string>iPhone Distribution</string>
    
    <!-- Signing style -->
    <key>signingStyle</key>
    <string>manual</string>
</dict>
</plist>