  "tokenColors": [
    {
      "name": "Noodle Language Tokens",
      "scope": [
        "keyword.control.noodle",
        "storage.type.noodle",
        "constant.language.noodle"
      ],
      "settings": {
        "foreground": "#c586c0c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function.noodle",
        "support.function.noodle"
      ],
      "settings": {
        "foreground": "#61dafb"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other.noodle",
        "variable.language.noodle"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.noodle",
        "string.unquoted.noodle"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric.noodle"
      ],
      "settings": {
        "foreground": "#d19a66"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment.line.noodle",
        "comment.block.noodle"
      ],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type.noodle",
        "support.type.noodle"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "AI Functions",
      "scope": [
        "support.function.ai.noodle",
        "entity.name.function.ai.noodle"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": "bold"
      }
    }
