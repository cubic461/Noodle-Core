  "tokenColors": [
    {
      "name": "Noodle Language Tokens",
      "scope": [
        "keyword.control.noodle",
        "storage.type.noodle",
        "constant.language.noodle"
      ],
      "settings": {
        "foreground": "#9b59b6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function.noodle",
        "support.function.noodle"
      ],
      "settings": {
        "foreground": "#2980b9"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable.other.noodle",
        "variable.language.noodle"
      ],
      "settings": {
        "foreground": "#d35400"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.noodle",
        "string.unquoted.noodle"
      ],
      "settings": {
        "foreground": "#27ae60"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric.noodle"
      ],
      "settings": {
        "foreground": "#d35400"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment.line.noodle",
        "comment.block.noodle"
      ],
      "settings": {
        "foreground": "#7f8c8d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type.noodle",
        "support.type.noodle"
      ],
      "settings": {
        "foreground": "#e67e22"
      }
    },
    {
      "name": "AI Functions",
      "scope": [
        "support.function.ai.noodle",
        "entity.name.function.ai.noodle"
      ],
      "settings": {
        "foreground": "#16a085",
        "fontStyle": "bold"
      }
    }
  ]
