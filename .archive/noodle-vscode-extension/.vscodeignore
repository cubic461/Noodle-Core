# VS Code extension ignore file
# This file tells VS Code which files and folders to exclude when packaging the extension

# Source files (compiled to out/ directory)
src/
*.ts
!*.d.ts

# Test files and directories
test/
tests/
**/*.test.ts
**/*.spec.ts
jest.config.js
jest.integration.config.js
jest.ui.config.js
jest.performance.config.js

# Development configuration files
webpack.config.js
tsconfig.json
.eslintrc.*
.prettierrc.*
.editorconfig

# Documentation files that aren't needed in the package
CONTRIBUTING.md
TEST_DOCUMENTATION.md
docs/
*.md
!README.md

# Dependency directories
node_modules/
.npm
.yarn

# Build output directories (out/ is needed for packaging)
out/test/
dist/
build/
lib/

# Environment and configuration files
.env
.env.*
.vscode/
!.vscode/extensions.json

# Version control
.git/
.gitignore
.gitattributes

# CI/CD files
.github/
.travis.yml
.circleci/
appveyor.yml
azure-pipelines.yml

# Package manager files
package-lock.json
yarn.lock
pnpm-lock.yaml

# IDE files
.vscode/settings.json
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Coverage reports
coverage/
*.lcov
.nyc_output

# Temporary files
tmp/
temp/
*.tmp
*.temp

# Development artifacts
*.tgz
*.tar.gz
.vscode-test/

# Local configuration
local.json
local.*.json